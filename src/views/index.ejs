</div>
 


 
    
    <div class="row">
      <div class="col-md-12">
        <h1 style="text-align: center; margin-top:70px; text-decoration: underline; text-underline-position: under;">Destacados</h1>
     </div>
</div>




<div class="site-section site-section-sm bg-light">
 <div class="container">
  <div class="row mb-5">
    <% ofertauno.forEach(function (ofertaun) { %>
    <div class="col-md-6 col-lg-4 mb-4">
      <div class="property-entry h-100">
        <a href="/ofertaunoredirect/<%= ofertaun.id %>" class="property-thumbnail">
          <div class="offer-type-wrap">
           </div>
          <img src="<%= ofertaun.imageuno %>" alt="Image" class="img-fluid">
        </a>
        <div class="p-4 property-body">
             
            <h2 class="property-title"><%= ofertaun.name %></h2>
            <span class="property-location d-block mb-3"><span class="property-icon icon-room"></span> <%= ofertaun.title %></span>
            <strong class="property-price text-primary mb-3 d-block text-success">$<%= ofertaun.price %></strong>
            <ul class="property-specs-wrap mb-3 mb-lg-0">
              <li>
                <span class="property-specs"> 
                  <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1656340397/182657_ycmmol.png">
                   
                </span>
                <span class="property-specs-number"><%= ofertaun.enstock %></span>
                
              </li>
              <li>
                <span class="property-specs">  
                   <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1661376513/WhatsApp_Image_2022-08-24_at_6.26.21_PM_jbaisz.jpg" alt="">
                </span>
                <span class="property-specs-number"><%= ofertaun.colorstock %></span>
                
              </li>
             
              

              <li>
                  <span class="property-specs"> 
                     <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1656178991/square-metre-outline-information-icon-260nw-2011557692_bl7q40.webp" alt="">
                    </span>
                  <span class="property-specs-number"><%= ofertaun.talle %></span>
                  
                </li>


              
            </ul>

          </div>
        </div>
      </div>
    <% }) %>


  </div>
</div>
</div>

 
    

