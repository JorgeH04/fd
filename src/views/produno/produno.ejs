  <div class="slide-one-item home-slider owl-carousel"  >

    <div class="site-blocks-cover overlay" style="background-image: url(https://res.cloudinary.com/dsk4fft6r/image/upload/v1661362629/BANNER_4_gyo0ce.jpg);" data-aos="fade" data-stellar-background-ratio="0.5">
      <div class="container">
